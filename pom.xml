            </plugin>

            <!-- Quick & dirty hack to support coverage with Jacoco, whenever 
                you modify these settings you have to copy paste changes to unit-test-coverage-profile, 
                as surefire plugin doesn't handle properly argLine argument which is set 
                by jacoco agent -->


            <!-- This plugin is used to provide possibility to run single 
                test. -->
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <skipTests>${skipUTs}</skipTests>
                    <argLine>-Xms256m -Xmx512m</argLine>
                    <systemPropertyVariables>
                        <logback.configurationFile>file:${config.dir}/${logback.testfile}</logback.configurationFile>
                    </systemPropertyVariables>
                    <excludes>
                        <exclude>**/*IT.java</exclude>
                        <exclude>**/*IntegrationTest.java</exclude>
                        <exclude>**/*DAOTest.java</exclude>
                        <exclude>**/*WithEntityManagerTest.java</exclude>
                    </excludes>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-failsafe-plugin</artifactId>
                <executions>
                    <execution>
                        <id>integration-test</id>
                        <goals>
                            <goal>integration-test</goal>
                            <goal>verify</goal>
                        </goals>
                        <configuration>
