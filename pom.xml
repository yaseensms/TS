                                <include>**/*IntegrationTest.java</include>
                                <include>**/*DAOTest.java</include>
                                <include>**/*WithEntityManagerTest.java</include>
                            </includes>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>

